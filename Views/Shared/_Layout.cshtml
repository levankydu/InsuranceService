
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title>Insurance - @ViewData["Title"]</title>
	<meta content="width=device-width, initial-scale=1.0" name="viewport" />
	<meta content="" name="keywords" />
	<meta content="" name="description" />

	<!-- Favicon -->
	<link href="~/HomeLibrary/img/favicon.ico" rel="icon" />

	<!-- Google Web Fonts -->
	<link rel="preconnect" href="https://fonts.googleapis.com" />
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
	<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&family=Poppins:wght@600;700&display=swap"
		  rel="stylesheet" />

	<!-- Icon Font Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
	<link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css"
		  rel="stylesheet" />

	<!-- Libraries Stylesheet -->
	<link href="~/HomeLibrary/animate/animate.min.css" rel="stylesheet" />
	<link href="~/HomeLibrary/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" />

	<!-- Customized Bootstrap Stylesheet -->
	<link href="~/HomeLibrary/css/bootstrap.min.css" rel="stylesheet" />

	<!-- Template Stylesheet -->
	<link href="~/HomeLibrary/css/style.css" rel="stylesheet" />
	@RenderSection("Css",required:false)
</head>

<body>
	<!-- Spinner Start -->
	<div id="spinner"
		 class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
		<div class="spinner-grow text-primary" role="status"></div>
	</div>
	<!-- Spinner End -->
	<!-- Topbar Start -->
	<div class="container-fluid bg-dark text-white-50 py-2 px-0 d-none d-lg-block">
		<div class="row gx-0 align-items-center">
			<div class="col-lg-7 px-5 text-start">
				<div class="h-100 d-inline-flex align-items-center me-4">
					<small class="fa fa-phone-alt me-2"></small>
					<small>
						<a href="tel:+84 8888 6666" style="color:rgba(255,255,255,0.5)!important;text-decoration:none">+84 8888 6666</a>
					</small>
				</div>
				<div class="h-100 d-inline-flex align-items-center me-4">
					<small class="far fa-envelope-open me-2"></small>
					<small>
						<a href="mailto:InsuranceSer@insser.com" style="color:rgba(255,255,255,0.5)!important;text-decoration:none">InsuranceService@insurance.com</a>

					</small>
				</div>
				<div class="h-100 d-inline-flex align-items-center me-4">
					<small class="far fa-clock me-2"></small>
					<small>Mon - Fri : 08 AM - 09 PM</small>
				</div>
			</div>
			<div class="col-lg-5 px-5 text-end">
				<div class="h-100 d-inline-flex align-items-center">
					<a class="text-white-50 ms-4" href="">
						<i class="fab fa-facebook-f"></i>
					</a>
					<a class="text-white-50 ms-4" href="">
						<i class="fab fa-twitter"></i>
					</a>
					<a class="text-white-50 ms-4" href="">
						<i class="fab fa-linkedin-in"></i>
					</a>
					<a class="text-white-50 ms-4" href="">
						<i class="fab fa-instagram"></i>
					</a>
					@if (!User.Identity!.IsAuthenticated)
					{
						<a href="/Login/Welcome" align="right" class="text-white-50 ms-4">Login</a>
					}

					@if (User.Identity.IsAuthenticated)
					{
						@if (User.IsInRole("user"))
						{
							<a href="/lifeInsuranceHolder" class="text-white-50 ms-4">Subscriptions</a>
							<a asp-action="Profile" asp-route-id="@User.Identity.Name" asp-controller="Home" class="text-white-50 ms-4">Profile</a>
						}

						@if (User.IsInRole("admin"))
						{
							<a asp-action="Profile" asp-route-id="@User.Identity.Name" asp-controller="Home" class="text-white-50 ms-4">Profile</a>
							<a href="/Admin/Display" class="text-white-50 ms-4">Admin Page</a>
						}
						<a id="logout-button" href="#" align="right" class="text-white-50 ms-4">Logout</a>
					}
				</div>
			</div>
		</div>
	</div>
	<!-- Topbar End -->
	<!-- Navbar Start -->
	<nav class="navbar navbar-expand-lg bg-white navbar-light sticky-top px-4 px-lg-5">
		<a href="/Home/index" class="navbar-brand d-flex align-items-center">
			<h1 class="m-0">
				<img class="img-fluid me-3"
					 src="~/HomeLibrary/img/icon/icon-02-primary.png"
					 alt="" />Insurance Service
			</h1>
		</a>
		<button type="button"
				class="navbar-toggler"
				data-bs-toggle="collapse"
				data-bs-target="#navbarCollapse">
			<span class="navbar-toggler-icon"></span>
		</button>
		<div class="collapse navbar-collapse" id="navbarCollapse">
			<div class="navbar-nav mx-auto bg-light rounded pe-4 py-3 py-lg-0">
				<a data-header-name="Home" asp-action="index" asp-controller="Home" class="nav-item nav-link">Home</a>
				<a data-header-name="Service" asp-action="OurService" asp-controller="Home" class="nav-item nav-link">Our Services</a>
				<a data-header-name="About" asp-action="AboutUs" asp-controller="Home" class="nav-item nav-link">About Us</a>
				<a data-header-name="Contact" asp-action="ContactUs" asp-controller="Home" class="nav-item nav-link">Contact Us</a>
			</div>
		</div>

	</nav>
	<!-- Navbar End -->
	<div>
		<main role="main">
			@RenderBody()


		</main>
	</div>

	<!-- Footer Start -->
	<div class="container-fluid bg-dark footer mt-5 pt-5 wow fadeIn"
		 data-wow-delay="0.1s">
		<div class="container py-5">
			<div class="row g-5">
				<div class="col-lg-3 col-md-6">
					<h1 class="text-white mb-4">
						<img class="img-fluid me-3"
							 src="~/HomeLibrary/img/icon/icon-02-light.png"
							 alt="" />Insurance
					</h1>
					@* <p>
						Insurance Service is not just a financial product. It's a promise of security and stability. It provides individuals and families with the confidence to pursue their dreams, knowing that they have a partner to rely on when challenges arise.
					</p> *@
					<div class="d-flex pt-2">
						<a class="btn btn-square me-1" href="">
							<i class="fab fa-twitter"></i>
						</a>
						<a class="btn btn-square me-1" href="">
							<i class="fab fa-facebook-f"></i>
						</a>
						<a class="btn btn-square me-1" href="">
							<i class="fab fa-youtube"></i>
						</a>
						<a class="btn btn-square me-0" href="">
							<i class="fab fa-linkedin-in"></i>
						</a>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
					<h5 class="text-light mb-4">Address</h5>
					<p>
						<i class="fa fa-map-marker-alt me-3"></i>688 Cach Mang Thang 8, Ho Chi Minh, Viet Nam
					</p>
					<p><i class="fa fa-phone-alt me-3"></i>+84 8888 6666</p>
					<p><i class="fa fa-envelope me-3"></i>InsuranceSer@insser.com</p>
				</div>
				<div class="col-lg-3 col-md-6">
					<h5 class="text-light mb-4">Quick Links</h5>
					<a class="btn btn-link" asp-action="index" asp-controller="Home">InsuranceService Home</a>
					<a class="btn btn-link" asp-action="OurService" asp-controller="Home">Our Services</a>
					<a class="btn btn-link" asp-action="AboutUs" asp-controller="Home">About Us</a>
					<a class="btn btn-link" asp-action="ContactUs" asp-controller="Home">Contact Us</a>

					@*     <a class="btn btn-link" href="">Terms & Condition</a> *@
					@*  <a class="btn btn-link" href="">Support</a> *@
				</div>
				<div class="col-lg-3 col-md-6">
					<h5 class="text-light mb-4">News</h5>
					<p>
						Insurance Service is not just a financial product. It's a promise of security and stability. It provides individuals and families with the confidence to pursue their dreams, knowing that they have a partner to rely on when challenges arise.
					</p>
					@* <div class="position-relative mx-auto" style="max-width: 400px">
						<input class="form-control bg-transparent w-100 py-3 ps-4 pe-5"
							   type="text"
							   placeholder="Your email" />
						<button type="button"
								class="btn btn-secondary py-2 position-absolute top-0 end-0 mt-2 me-2">
							SignUp
						</button>
					</div> *@
				</div>
			</div>
		</div>
		<div class="container-fluid copyright">
			<div class="container">
				<div class="row">
					<div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
						&copy; <a asp-action="index" asp-controller="Home">InsuranceService</a>, All Right Reserved.
					</div>

				</div>
			</div>
		</div>
	</div>
	<!-- Footer End -->
	<!-- Back to Top -->
	<a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top">
		<i class="bi bi-arrow-up"></i>
	</a>

	<!-- JavaScript Libraries -->
	<script type="text/javascript" src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
	<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/HomeLibrary/wow/wow.min.js"></script>
	<script src="~/HomeLibrary/easing/easing.min.js"></script>
	<script src="~/HomeLibrary/waypoints/waypoints.min.js"></script>
	<script src="~/HomeLibrary/owlcarousel/owl.carousel.min.js"></script>
	<script src="~/HomeLibrary/counterup/counterup.min.js"></script>
	<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>


	<!-- Template Javascript -->
	<script src="~/HomeLibrary/js/main.js"></script>
	<script src="~/HomeLibrary/js/custom.js"></script>
	<script>
		const logoutBtn = document.querySelector("#logout-button");
		logoutBtn.addEventListener("click", function (e) {
			e.preventDefault();
			swal({
				title: "Logging out, are you sure?",
				icon: "info",
				buttons: true,
				dangerMode: true,
			})
				.then((isAccept) => {
					if (isAccept) {
						window.location.href = '/Login/Logout';
					}
				});
		});
	</script>
	@RenderSection("JS",required:false)
</body>
</html>
