@model List<test0000001.Models.Question>

@{
    ViewData["Title"] = "Questions";
    Layout = "AdminLayout/_Layout";
}

<div class="page-heading">
    <div class="page-title">
        <div class="row">
            <div class="col-12 col-md-6 order-md-1 order-last">
                <h3>Health Policy</h3>
                <p class="text-subtitle text-muted">
                    All Health Policy Question is here
                </p>
                <a asp-action="QuestionCreate" asp-controller="HealthInsurance" class="btn btn-primary">Create</a>
            </div>
            <div class="col-12 col-md-6 order-md-2 order-first">
                <nav aria-label="breadcrumb"
                     class="breadcrumb-header float-start float-lg-end">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a asp-action="Display" asp-controller="Admin">Dashboard</a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">
                            List Health Policy Question
                        </li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>

    <!-- Basic Tables start -->
    <section class="section">
        <div class="card">
            <div class="card-header">Health Policy</div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table" id="table1">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Content</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.Id</td>
                                    <td>@item.Content?</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
    <!-- Basic Tables end -->
</div>